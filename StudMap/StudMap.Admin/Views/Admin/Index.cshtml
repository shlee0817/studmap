@model Object
@{
    ViewBag.Title = "Admin";
}
@section styles{
    <link rel="stylesheet" href="@Url.Content("~/Content/Home/Admin.css")" type="text/css"/>
}
@section scripts
{
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-ui-1.10.3.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/d3.v3.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/d3.floorplan.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Admin/Admin.js")"></script>
}

@{
    string viewName = ViewBag.PartialViewName ?? "_Maps";
}

<div>
    <div id="mapsTab" class="tab @(viewName.Equals("_Maps") ? "active" : "")">Maps</div>
    <div id="floorsTab" class="tab @(viewName.Equals("_Floors") ? "active" : "")">Floors</div>
    <div id="layerTab" class="tab @(viewName.Equals("_Layer") ? "active" : "")">Layer</div>
    <br style="clear:both"/>
</div>
<div id="adminContent">
    @{
        Html.RenderPartial(viewName, Model);
    }
</div>

<div class="modal"></div>

<div id="NodeInformationDialog"></div>

<div id="DeleteDialog"></div>